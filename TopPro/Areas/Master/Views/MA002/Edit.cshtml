@model TopPro.Domain.Model.MA002
@{
    ViewBag.Title = "Edit";
    Layout = "~/Areas/Master/Views/Shared/_Layout.cshtml";
}
<!-- End Navbar -->
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-8">
                <div class="card">
                    <div class="card-header card-header-primary">
                        <h4 class="card-title">Edit Profile</h4>
                        <p class="card-category">Complete your profile</p>
                    </div>
                    <div class="card-body">
                        @using (Html.BeginForm("Edit", "UserMaster", FormMethod.Post))
                        {@Html.HiddenFor(m => m.Id)
                        @Html.HiddenFor(m => m.MBUSRCD)

                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSRCD, htmlAttributes: new { @class = "bmd-label-floating" })
                            @Html.DisplayFor(m => m.MBUSRCD)
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSRNM, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.DisplayFor(m => m.MBUSRNM)

                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSSNM, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUSSNM, new { @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.MBUSSNM, "", new { @class = "text-danger" })

                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSAD1, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUSAD1, new { @class = "form-control" })

                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSAD2, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUSAD2, new { @class = "form-control" })

                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSAD3, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUSAD3, new { @class = "form-control" })

                        </div>


                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUSTEL, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUSTEL, new { @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.MBUSTEL, "", new { @class = "text-danger" })

                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUCC, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUCC, new { @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.MBUCC, "", new { @class = "text-danger" })
                            <div class="col-sm-12 text-action">( (1:MM 2:INCH) )</div>

                        </div>
                        <div class="form-group">
                            @Html.LabelFor(m => m.MBUUC, htmlAttributes: new { @class = "bmd-label-floating" })

                            @Html.TextBoxFor(m => m.MBUUC, new { @class = "form-control" })
                            @Html.ValidationMessageFor(m => m.MBUUC, "", new { @class = "text-danger" })
                            <div class="col-sm-12 text-action">( 1:KG 2:POUND )</div>

                        </div>


                    }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
